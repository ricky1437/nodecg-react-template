{
	"name": "nodecg-react-template",
	"version": "1.0.0",
	"scripts": {
		"dev": "run-p dev:*",
		"dev:tsc": "tsc -b src/browser src/extension -w --preserveWatchOutput",
		"dev:vite": "vite",
		"dev:nodecg": "npx nodecg start",
		"generate-schema-types": "nodecg schema-types -o ./src/nodecg/generated",
		"build": "vite build"
	},
	"nodecg": {
		"compatibleRange": "^2.0.0",
		"assetCategories": [
			{
				"name": "test",
				"title": "Test",
				"allowedTypes": [
					"jpg",
					"jpeg",
					"gif",
					"png"
				]
			}
		],
		"dashboardPanels": [
			{
				"name": "example",
				"title": "Example",
				"width": 2,
				"file": "example.html",
				"workspace": "Example",
				"headerColor": "#525F78"
			}
		],
		"graphics": [
			{
				"file": "example.html",
				"width": 1920,
				"height": 1080,
				"singleInstance": false
			}
		]
	},
	"devDependencies": {
		"@nodecg/types": "^2.1.12",
		"@types/react": "^18.3.8",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"globby": "^14.0.2",
		"npm-run-all": "^4.1.5",
		"prettier-plugin-organize-imports": "^4.2.0",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-node-externals": "^7.1.3",
		"trash-cli": "^6.0.0",
		"ts-nodecg": "^0.3.1",
		"typescript": "^5.6.2",
		"vite": "^5.4.8"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mdi/react": "^1.6.1",
		"@mui/material": "^7.3.1",
		"@nodecg/react-hooks": "^1.0.3",
		"nodecg": "^2.6.1",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tslib": "^2.8.1"
	}
}
